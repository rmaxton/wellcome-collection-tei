                        <textLang mainLang="sa" source="IANA">Sanskrit</textLang>

                        <msItem n="1" xml:id="E.11.a_1">
                            <locus from="f.1v" to="f.14v">F.1v-14v </locus>
                            <title> A Cāturmāsyahautraprayoga</title>
                            <incipit><locus>F.1v </locus>śrīgaṇeśāya namaḥ || atha
                                cāturmāsya-hautraprayogaḥ || tatra prathamā cāraṃbhaṇīyeṣṭiḥ || nama
                                pravaktra ityādi...</incipit>
                            <explicit><locus>F.14v </locus>...saṃtiṣṭhate cāturmāsyāni || || iti
                                cāturmāsyahautraḥ samāptaḥ ||</explicit>
                        </msItem>

                        <msItem n="2" xml:id="E.11.a_2">
                            <locus from="f.15" to="f.15v">F.15-15v </locus>
                            <title>A Kārīrīṣṭihautraprayoga</title>
                            <incipit><locus>F.15 </locus>śrīgaṇeśāya namaḥ || atha kārīrīṣṭihaut
                                prayogaḥ || namaḥ pravaktra ityādi ||</incipit>
                            <explicit><locus>F.15v </locus>...pratyṛcaṃ sūktena suktena vā || iti
                                kārīrīṣṭihautraprayogaḥ ||</explicit>
                        </msItem>

